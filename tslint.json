{
  "defaultSeverity": "error",
  "extends": [
    "tslint-config-airbnb",
    "tslint-react"
  ],
  "jsRules": {},
  "rules": {
    "import-name": [
      true,
      {
        "react": "React",
        "reactDom": "ReactDOM",
        "logoSvg": "logo"
      }
    ],
    "variable-name": {
      "options": [
        "ban-keywords",
        "check-format",
        "allow-pascal-case"
      ]
    }
  },
  "rulesDirectory": []
}
